# Bot Simulation Configuration
# Copy this file to .env and modify the values as needed

# URL to visit during bot simulation
URL=[your_cloudfront_or_dns_with_route]

# Run browser in headless mode (true/false)
HEADLESS=true

# Number of times to visit the URL
ITERATIONS=3

# Delay between visits in seconds
DELAY=1

# Browser timeout in seconds
TIMEOUT=30
